app.controller('galleryController', function($scope, News, $ionicLoading, Constant){
    
    $ionicLoading.show({template: Constant.loadingTemplate});
    
    News.getGallery().then(function(response) {
        console.log('galleryController', response);
        $scope.gallery = response;
        $ionicLoading.hide();
    });
    
});
